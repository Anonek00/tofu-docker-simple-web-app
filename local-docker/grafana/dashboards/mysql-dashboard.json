{
  "id": null,
  "uid": "mysql-dashboard",
  "title": "MySQL Database Monitoring",
  "description": "Monitoring dashboard for MySQL with Prometheus metrics",
  "tags": ["mysql", "database", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 37,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "MySQL Uptime",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "mysql_global_status_uptime",
          "legendFormat": "Uptime",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 2,
      "title": "Queries per Second",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate(mysql_global_status_queries[5m])",
          "legendFormat": "Queries/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "qps",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 3,
      "title": "Connections",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "mysql_global_status_threads_connected",
          "legendFormat": "Connected",
          "refId": "A"
        },
        {
          "expr": "mysql_global_status_max_used_connections",
          "legendFormat": "Max Used",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 4,
      "title": "InnoDB Buffer Pool Size",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "mysql_global_variables_innodb_buffer_pool_size",
          "legendFormat": "Buffer Pool Size",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 5,
      "title": "Query Types",
      "type": "timeseries", 
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "rate(mysql_global_status_com_select[5m])",
          "legendFormat": "SELECT",
          "refId": "A"
        },
        {
          "expr": "rate(mysql_global_status_com_insert[5m])",
          "legendFormat": "INSERT",
          "refId": "B"
        },
        {
          "expr": "rate(mysql_global_status_com_update[5m])",
          "legendFormat": "UPDATE",
          "refId": "C"
        },
        {
          "expr": "rate(mysql_global_status_com_delete[5m])",
          "legendFormat": "DELETE",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "qps",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 6,
      "title": "Slow Queries",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "rate(mysql_global_status_slow_queries[5m])",
          "legendFormat": "Slow Queries/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "qps",
          "color": {"mode": "palette-classic"}
        }
      }
    }
  ]
}