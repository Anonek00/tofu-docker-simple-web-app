{
  "id": null,
  "uid": "redis-dashboard",
  "title": "Redis Monitoring Dashboard",
  "description": "Monitoring dashboard for Redis with Prometheus metrics",
  "tags": ["redis", "cache", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 37,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Redis Uptime",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "redis_uptime_in_seconds",
          "legendFormat": "Uptime",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 2,
      "title": "Connected Clients",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "redis_connected_clients",
          "legendFormat": "Connected Clients",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 3,
      "title": "Memory Usage",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "redis_memory_used_bytes",
          "legendFormat": "Used Memory",
          "refId": "A"
        },
        {
          "expr": "redis_memory_max_bytes",
          "legendFormat": "Max Memory",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 4,
      "title": "Total Keys",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "redis_db_keys",
          "legendFormat": "Total Keys",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 5,
      "title": "Commands per Second",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "rate(redis_commands_processed_total[5m])",
          "legendFormat": "Commands/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 6,
      "title": "Cache Hit/Miss Ratio",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "rate(redis_keyspace_hits_total[5m])",
          "legendFormat": "Cache Hits",
          "refId": "A"
        },
        {
          "expr": "rate(redis_keyspace_misses_total[5m])",
          "legendFormat": "Cache Misses",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {"mode": "palette-classic"}
        }
      }
    }
  ]
}